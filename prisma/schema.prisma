// Ini adalah komentar untuk menjelaskan skema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tag {
  id        String   @id @default(uuid())
  name      String
  color     String
  status    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
